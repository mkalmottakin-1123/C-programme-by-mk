#include <stdio.h>
int main(){
    int a[2][3]={{3,1,2},{9,7,8}}, temp;
    for(int i=0;i<2;i++){
        for(int j=0;j<3-1;j++)
            for(int k=j+1;k<3;k++)
                if(a[i][j]>a[i][k]){
                    temp=a[i][j];
                    a[i][j]=a[i][k];
                    a[i][k]=temp;
                }
    }
    for(int i=0;i<2;i++){
        for(int j=0;j<3;j++) printf("%d ",a[i][j]);
        printf("\n");
    }
    return 0;
}
