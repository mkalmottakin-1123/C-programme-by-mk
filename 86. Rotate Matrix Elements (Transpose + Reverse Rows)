#include <stdio.h>
int main(){
    int a[2][2]={{1,2},{3,4}}, temp;
    // Transpose
    for(int i=0;i<2;i++)
        for(int j=i;j<2;j++){
            temp=a[i][j];
            a[i][j]=a[j][i];
            a[j][i]=temp;
        }
    // Reverse each row
    for(int i=0;i<2;i++){
        temp=a[i][0]; a[i][0]=a[i][1]; a[i][1]=temp;
    }
    printf("Rotated Matrix:\n");
    for(int i=0;i<2;i++){
        for(int j=0;j<2;j++) printf("%d ",a[i][j]);
        printf("\n");
    }
    return 0;
}
