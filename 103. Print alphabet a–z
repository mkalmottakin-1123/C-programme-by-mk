#include <stdio.h>
#include <string.h>

int main() {
    char a[100], b[100];

    printf("Enter first string: ");
    gets(a);
    printf("Enter second string: ");
    gets(b);

    int result = strcmp(a, b);

    if (result == 0)
        printf("Both strings are equal");
    else if (result < 0)
        printf("First string is smaller");
    else
        printf("First string is greater");

    return 0;
}
