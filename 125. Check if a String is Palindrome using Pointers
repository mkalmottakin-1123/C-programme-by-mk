#include <stdio.h>
#include <string.h>
int main(){
    char s[100];
    printf("Enter string: ");
    if(scanf("%99s", s)!=1) return 0;
    char *l = s, *r = s + strlen(s) -1;
    int ok = 1;
    while(l<r){ if(*l!=*r){ ok=0; break; } l++; r--; }
    printf("%s\n", ok ? "Palindrome" : "Not palindrome");
    return 0;
}
