#include <stdio.h>
#include <string.h>

int main() {
    char str1[200], str2[100], result[300];
    int pos, i, j;

    printf("Main string: ");
    gets(str1);
    printf("String to insert: ");
    gets(str2);
    printf("Position: ");
    scanf("%d", &pos);

    // Copy first part
    for (i = 0; i < pos; i++)
        result[i] = str1[i];

    // Insert second string
    for (j = 0; str2[j] != '\0'; j++)
        result[i++] = str2[j];

    // Copy remaining part
    for (j = pos; str1[j] != '\0'; j++)
        result[i++] = str1[j];

    result[i] = '\0';

    printf("Result: %s", result);
    return 0;
}
