#include <stdio.h>
int main() {
    int a[] = {9, 4, 6, 2, 1}, n = 5, temp;
    for(int i=0; i<n-1; i++)
        for(int j=i+1; j<n; j++)
            if(a[i] > a[j]) {
                temp = a[i];
                a[i] = a[j];
                a[j] = temp;
            }
    printf("Ascending order: ");
    for(int i=0; i<n; i++) printf("%d ", a[i]);
    return 0;
}
