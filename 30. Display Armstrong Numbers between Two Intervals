#include <stdio.h>
#include <math.h>
int main() {
    int low, high, n, r, sum, temp, digits;
    printf("Enter two numbers: ");
    scanf("%d %d", &low, &high);
    for (n=low; n<=high; n++) {
        temp=n; digits=0; sum=0;
        while(temp!=0){ digits++; temp/=10; }
        temp=n;
        while(temp!=0){
            r=temp%10;
            sum+=pow(r,digits);
            temp/=10;
        }
        if(sum==n) printf("%d ", n);
    }
    return 0;
}
