#include <stdio.h>
#include <math.h>
int main() {
    int n, r, sum=0, temp, digits=0;
    printf("Enter number: ");
    scanf("%d", &n);
    temp=n;
    while(temp!=0){ digits++; temp/=10; }
    temp=n;
    while(temp!=0){
        r=temp%10;
        sum+=pow(r,digits);
        temp/=10;
    }
    if(sum==n) printf("Armstrong");
    else printf("Not Armstrong");
    return 0;
}
